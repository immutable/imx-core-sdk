{
  "name": "imx-core-sdk-examples",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "register-user": "ts-node -r dotenv/config registerUser.ts",
    "create-order": "ts-node -r dotenv/config createOrder.ts",
    "list-orders": "ts-node -r dotenv/config listOrders.ts",
    "get-order": "ts-node -r dotenv/config getOrder.ts",
    "cancel-order": "ts-node -r dotenv/config cancelOrder.ts",
    "create-trade": "ts-node -r dotenv/config createTrade.ts",
    "list-trades": "ts-node -r dotenv/config listTrades.ts",
    "get-trade": "ts-node -r dotenv/config getTrade.ts",
    "create-exchange-transfer": "ts-node -r dotenv/config createExchangeTransfer.ts",
