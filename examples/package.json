    "@imtbl/core-sdk": "^1.0.0",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.3"
  }
